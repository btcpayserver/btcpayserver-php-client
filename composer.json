{
  "name": "petzsch/btcpayserver-php-client",
  "description": "PHP Library to work with the new cryptographically secure BTCPayServer API",
  "license": "MIT",
  "minimum-stability": "stable",
  "keywords": [
    "btcpayserver",
    "bitcoin",
    "cash",
    "payment",
    "gateway"
  ],
  "homepage": "https://github.com/petzsch/btcpayserver-php-client",
  "require": {
    "bitpay/key-utils": "^1.0",
    "php": "^7.1",
    "guzzlehttp/guzzle": "^6.3",
    "ext-json": "*",
    "ext-reflection": "*",
    "symfony/yaml": "^3.0 || ^4.3"
  },
  "authors": [
    {
      "name": "BTCPayServer Developer Community",
      "homepage": "https://github.com/btcpayserver"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BtcPaySDK\\Test\\": "tests/"
    }
  }
}
